{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "remix build && run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "generate:css": "npx tailwindcss -i ./app/root.css -o ./app/tailwind.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.6.1",
    "@remix-run/cloudflare-pages": "^1.6.1",
    "@remix-run/node": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "@remix-run/serve": "^1.6.1",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.13.0",
    "@remix-run/dev": "^1.6.1",
    "@remix-run/eslint-config": "^1.6.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  }
}
